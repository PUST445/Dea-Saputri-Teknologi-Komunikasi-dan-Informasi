.dataTables_paginate { float: right; margin: 4px 0px 0px; }

.dataTables_paginate .paginate_button { color: rgb(17, 17, 17); display: inline-block; margin: 0px 5px; outline: none; position: relative; text-decoration: underline; }

.dataTables_paginate .paginate_button:first-child { margin-left: 0px; }

.dataTables_paginate .paginate_button:last-child { margin-right: 0px; }

.dataTables_paginate .paginate_button::after, .dataTables_paginate .paginate_button::before { color: rgb(217, 237, 247); }

.dataTables_paginate .paginate_button:hover { cursor: pointer; text-decoration: none; }

.dataTables_paginate .paginate_button:hover::after, .dataTables_paginate .paginate_button:hover::before { color: rgb(4, 156, 219); }

.dataTables_paginate .paginate_button.disabled { color: rgb(153, 153, 153); cursor: default; text-decoration: none; }

.dataTables_paginate .paginate_button.disabled::after, .dataTables_paginate .paginate_button.disabled::before { color: rgb(249, 249, 249); }

.dataTables_paginate .paginate_button.current { cursor: default; font-weight: 700; text-decoration: none; }

.dataTables_paginate.paging_simple { padding: 0px 15px; }

.dataTables_paginate.paging_simple .paginate_button.next::after, .dataTables_paginate.paging_simple .paginate_button.previous::before { font-family: TablePress; font-size: 14px; font-weight: 700; text-align: left; -webkit-font-smoothing: antialiased; inset: 0px auto 0px -14px; content: ""; height: 14px; line-height: 1; margin: auto; position: absolute; text-shadow: rgb(102, 102, 102) 0.1em 0.1em; width: 14px; }

.dataTables_paginate.paging_simple .paginate_button.next::after { content: ""; left: auto; text-align: right; }

.dataTables_scroll { clear: both; }

.dataTables_scroll .tablepress { width: 100% !important; }

.dataTables_scrollHead table.tablepress { margin: 0px; }

.dataTables_scrollBody { }

.dataTables_scrollBody .tablepress thead th::after { content: ""; }

.dataTables_wrapper .dataTables_scroll div.dataTables_scrollBody td > div.dataTables_sizing, .dataTables_wrapper .dataTables_scroll div.dataTables_scrollBody th > div.dataTables_sizing { height: 0px; overflow: hidden; margin: 0px !important; padding: 0px !important; }

.tablepress { --head-active-bg-color: #049cdb; --head-active-text-color: var(--head-text-color); }

.tablepress .sorting, .tablepress .sorting_asc, .tablepress .sorting_desc { cursor: pointer; outline: none; padding-right: 20px; position: relative; }

.tablepress .sorting::after, .tablepress .sorting_asc::after, .tablepress .sorting_desc::after { font-family: TablePress; font-size: 14px; font-weight: 400; -webkit-font-smoothing: antialiased; inset: 0px 6px 0px auto; height: 14px; line-height: 1; margin: auto; position: absolute; }

.tablepress .sorting::after { content: ""; }

.tablepress .sorting_asc::after { content: ""; padding: 0px 0px 2px; }

.tablepress .sorting_desc::after { content: ""; }

.tablepress .sorting:hover, .tablepress .sorting_asc, .tablepress .sorting_desc { background-color: var(--head-active-bg-color); color: var(--head-active-text-color); }
