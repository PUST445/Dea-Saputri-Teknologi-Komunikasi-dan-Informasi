Content-Type: text/css
Content-Transfer-Encoding: binary
Content-Location: https://jurnalkwangsan.kemdikbud.go.id/public/site/sitestyle.css

@charset "utf-8";

@import url("//fonts.googleapis.com/css?family=PT+Sans:400,700&display=swap");

@import url("//fonts.googleapis.com/css?family=PT+Sans:400,700&display=swap");

#headerTitle, #navbar { width: 1024px; }

#header { background-color: transparent; padding-bottom: 40px; }

#navbar { background-color: rgb(22, 133, 204); background-repeat: repeat-x; border: 0px; height: 40px; margin-bottom: 10px; padding: 0px; text-align: justify; margin-left: 0px !important; }

#navbar ul { margin: 0px; padding: 0px 0px 0px 10px; }

#navbar ul li { float: left; margin: 0px; padding: 0px; }

#navbar a, #navbar a:link, #navbar a:visited { color: rgb(233, 219, 8); display: inline-block; font-size: 13px; font-weight: 700; line-height: 40px; margin: 0px; padding-left: 10px; padding-right: 10px; }

#navbar a::after, #navbar a::before { content: ""; display: block; height: 30px; position: absolute; top: 5px; width: 1px; }

#navbar a::before { margin-left: -10px; opacity: 0.15; }

#navbar a::after { margin-left: -9px; opacity: 0.35; }

#navbar li:first-child a::after, #navbar li:first-child a::before { background-color: rgb(0, 0, 0); display: none; }

#navbar a:hover { background: rgb(8, 81, 119); }

#body, #header, #main, #navbar, #sidebar { display: inline-block !important; }

#header, #main, #navbar, ul.sidemenu li::before { display: none; }

#body, #main, body { background-color: rgb(255, 255, 255); }

#additionalHomeContent, #announcementsHome, #navbar { clear: both; }

#container, #headerTitle { margin: 0px auto; display: inline-block; }

#announcements, #container { width: auto; }

#body, #container, #headerTitle, #headerTitle img { display: inline-block; }

body { color: rgb(51, 51, 51); font-family: "PT Sans"; font-size: 21px; }

a:active, a:link, a:visited { color: black; text-decoration: underline; }

a:hover { background: rgba(0, 0, 255, 0.2); }

#navbar a:active, a:hover { background: rgba(0, 0, 255, 0.1); text-decoration: underline; }

#container { background: url("https://jurnalkwangsan.kemdikbud.go.id/public/journals/1/kwangsan.jpg"); text-align: center; min-width: 730px !important; }

#headerTitle { width: 1020px; }

#header h1 { color: rgb(17, 17, 17); font-family: "PT+Sans"; font-size: 1.4em; font-weight: 400; letter-spacing: 1px; line-height: 130px; padding: 0px; text-shadow: rgb(255, 255, 255) 1px 1px; }

#headerTitle img { float: left; margin-right: 20px; }

#body { margin: -40px auto 0px; padding-bottom: 18px; width: 1020px !important; }

#main { float: left; margin-left: 0px; padding: 0px; position: relative; width: 805px; margin-right: 0px !important; overflow: visible !important; }

#content h1, #content h2, #content h3, #content h4, #main > h2 { color: rgb(51, 51, 51); font-family: "PT Sans"; font-weight: 700; }

#content { border-top: 0px dotted rgb(0, 0, 0); }

ul.sidemenu li a, ul.sidemenu li:first-child a { border-top: 1px solid rgb(255, 255, 255); }

ul.sidemenu { margin: -10px -10px 0px; padding-left: 0px !important; }

ul.sidemenu li { padding: 0px; }

ul.sidemenu li a { background: rgb(9, 150, 224); color: rgb(233, 219, 8); display: block; padding: 0.66em 10px 0.6em 14px; text-decoration: none; }

#leftSidebar { display: none; border-right: 1px solid rgb(204, 204, 204); float: left; width: 0px; }

ul.sidemenu li:last-child a { border-bottom: 0px; }

ul.sidemenu li a:hover { background-color: rgb(141, 144, 7); text-decoration: underline; }

#breadcrumb { margin: 0px 20px; padding: 0.7em 0px; border-bottom: 1px dotted rgb(51, 51, 51); }

#content, #main > h2 { padding-left: 20px; padding-right: 20px; }

#cpr { border: 0px rgb(255, 255, 255); }

#leftSidebar, #rightSidebar { font-size: 0.68em; height: 100%; margin-left: 0px; margin-right: 0px; overflow: hidden; padding: 50px 10px 10px; }

#rightSidebar { border-left: 1px solid rgb(204, 204, 204); float: right; width: 191px; background: 0px 0px; }

#leftSidebar div.block, #rightSidebar div.block { border: 0px; color: inherit; padding: 0px 0px 10px; margin-bottom: 10px; }

#leftSidebar div.block span.blockTitle, #rightSidebar div.block span.blockTitle, #sidebarDevelopedBy.block, #sidebarHelp.block { color: rgb(255, 255, 255); display: block; letter-spacing: 0px; margin-bottom: 10px; background: url("stripes.png") rgb(22, 133, 204); text-transform: uppercase; padding: 0.4em 10px !important; }

#sidebarDevelopedBy.block a, #sidebarHelp.block a { color: rgb(255, 255, 255); text-transform: uppercase; text-decoration: underline; }

#content h4 a, input[type="button"], input[type="submit"], table.tocArticle a { text-decoration: none; }

.block p { margin: 0px !important; }

.block ul { list-style: none; padding-left: 1.2em; }

.block li::before { content: "»"; display: block; height: 0px; left: -1em; position: relative; width: 0px; }

.banner-img, .indexers a, .intro { display: inline-block; }

#sidebarFontSize.block { padding-bottom: 35px !important; }

table.tocArticle:nth-child(2n+1) tr { background-color: rgb(240, 240, 255); }

table.tocArticle:nth-child(2n) tr { background-color: rgb(244, 244, 244); }

table.tocArticle tr:hover { background-color: rgb(255, 237, 204); }

table.tocArticle td { padding: 5px 10px; }

.tocTitle { font-weight: 700; }

td.tocGalleys { padding-right: 10px; }

#content ul.menu { margin: 0px; }

#content ul.menu li { background-image: linear-gradient(rgb(38, 81, 255) 0px, rgb(79, 114, 255) 100%); background-repeat: repeat-x; border-radius: 3px; padding: 5px 8px; }

#content ul.menu li.current, #content ul.menu li:hover { background-image: linear-gradient(rgb(236, 121, 15) 0px, rgb(242, 140, 49) 100%); background-repeat: repeat-x; }

#content ul.menu a { color: rgb(255, 255, 255); }

#content ul.menu a:hover { color: rgb(255, 255, 0); }

