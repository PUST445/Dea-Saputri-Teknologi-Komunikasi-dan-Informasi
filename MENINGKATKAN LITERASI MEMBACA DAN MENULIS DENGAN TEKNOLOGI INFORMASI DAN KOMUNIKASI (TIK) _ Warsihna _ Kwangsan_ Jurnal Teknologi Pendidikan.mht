@charset "utf-8";

@import url("./pdfView.css");

#body { width: 95%; margin: 0px auto; }

#container { min-width: 150px !important; }

#main { width: 100%; margin: 0px; }

#main h2 { font-size: 1em; }

#main h5 { margin: 0em 1em 0.5em 0em; padding-top: 0em; font-size: 1.3em; font-family: Georgia, "Times New Roman", Times, serif; font-weight: normal; }

#main h6 { margin: 0.5em 1em 0.5em 0em; padding: 0px; font-family: Georgia, "Times New Roman", Times, serif; font-weight: normal; font-size: 1.2em; }

#content { border: none; }

#content img.accessLogo { display: inline; vertical-align: middle; margin: 0px; padding: 0px; }

#topBar { width: 100%; padding-top: 1em; padding-bottom: 2.5em; }

#articleFontSize { float: right; }

#articleFontSize img { display: inline; vertical-align: middle; margin: 0px; padding: 0px; }

#accessKey { float: left; }

#accessKey img { display: inline; vertical-align: middle; margin: 0px; padding: 0px; }

#articleCoverImage { width: 100%; padding-top: 1em; padding-bottom: 1em; clear: left; }

#articleCoverImage img { display: block; border: 0px; }

div.rtSeparator { width: 100%; padding: 0px; border-bottom: 2px dotted rgb(0, 0, 0); line-height: 0px; }

div.rtSeparatorThin { width: 100%; padding: 5px 0px; border-bottom: 1px dotted rgb(0, 0, 0); line-height: 0px; }

div.rtBlock { }

div.rtBlock a { text-transform: none; text-decoration: none; }

div.rtBlock ul { padding: 0px; margin: 0px; }

div.rtBlock li { list-style-type: none; }

span.rtSubtitle { text-transform: uppercase; font-size: 0.85em; }

a.rtAction { font-size: 0.9em; text-transform: uppercase; text-decoration: none; }

table.bm { border: 1px solid rgb(0, 0, 0); }

.tl-default, .tl-main-part { font-weight: bold; }

dt { display: inline; float: left; margin-left: 20px; width: 80px; }

dd { margin-left: 100px; }

.articleToolItem { margin-bottom: 3px; }

.articleToolIcon { padding-right: 5px; vertical-align: middle; }

#sidebarRTRelatedItems ul { padding: 0px; }

#sidebarRTRelatedItems li { margin-bottom: 3px; }

#toggleRelatedItems { display: none; padding-top: 3px; cursor: pointer; }
