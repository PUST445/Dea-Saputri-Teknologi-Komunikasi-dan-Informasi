#inlinePdf { width: 99%; height: 99%; padding: 1px 0px 1px 1px; }

#inlinePdf.fullscreen { position: fixed; width: 97%; height: 95%; left: 20px; top: 7px; z-index: 1002; }

#pdfDownloadLinkContainer { text-align: right; padding: 5px; }

a.pdf { padding: 3px 0px 3px 23px; font-size: 1.2em; text-transform: none; background: url("../lib/pkp/templates/images/structure/page_white_acrobat.png") 0px 2px no-repeat scroll transparent !important; }

.black_overlay { display: none; position: absolute; top: 0%; left: 0%; width: 100%; height: 2000px; background-color: black; z-index: 1001; opacity: 0.7; }

#fullscreenShow { display: none; float: right; }

#fullscreenHide { padding: 2px; background-color: white; display: none; position: fixed; bottom: 3px; right: 3px; z-index: 1003; }
